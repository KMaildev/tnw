<script>
    function openNav() {
        document.getElementById("mySidenav").style.width = "95%";
        $(document).ready(function() {
            $(this).click(function() {
                $('#mySidenav').css("display", "none !important");
            });
        });
        
    }
    function closeNav() {
      document.getElementById("mySidenav").style.width = "0";
    }
</script>

